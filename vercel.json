{
    "version": 2,
    "framework": "vite",
    "buildCommand": "composer install --optimize-autoloader --no-dev && php artisan config:cache && php artisan route:cache",
    "outputDirectory": "public",
    "devCommand": "npm run dev",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://isosoft-web.vercel.app",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "localhost",
        "DB_PORT": "3306",
        "DB_DATABASE": "i_website",
        "DB_USERNAME": "root",
        "DB_PASSWORD": "root"
    }
}